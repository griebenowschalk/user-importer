{
  "name": "user-importer",
  "version": "1.0.0",
  "description": "Lightweight, modular importer for Users supporting CSV/XLS/XLSX/JSON with header mapping, validation, and scalable handling of large files",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "prepare": "husky",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest --coverage",
    "coverage:watch": "vitest --watch --coverage"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md,html}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "user-import",
    "csv",
    "excel",
    "data-validation",
    "react"
  ],
  "author": "Schalk Griebenow",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-table": "^8.20.0",
    "@tanstack/react-virtual": "^3.0.0",
    "@xstate/react": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "comlink": "^4.4.1",
    "date-fns": "^3.6.0",
    "lucide-react": "^0.541.0",
    "papaparse": "^5.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1",
    "xlsx": "^0.18.5",
    "xstate": "^5.20.2",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/fuse": "^2.5.2",
    "@types/node": "^24.3.0",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^4.0.5",
    "autoprefixer": "^10.4.21",
    "country-codes-list": "^2.0.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "fuse.js": "^7.1.0",
    "husky": "^9.1.7",
    "i18n-iso-countries": "^7.14.0",
    "jsdom": "^27.0.1",
    "libphonenumber-js": "^1.12.13",
    "lint-staged": "^16.1.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "react-country-flag": "^3.1.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.63.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^4.0.5"
  }
}
